{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Content-Security-Policy",
          "value": "connect-src 'self' http://209.97.174.250:3000 https://209.97.174.250:3000 wss: data: blob:"
        }
      ]
    }
  ]
}